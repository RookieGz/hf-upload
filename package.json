{
  "name": "hf-upload",
  "version": "1.0.5",
  "description": "An upload tool",
  "main": "./lib/index.js",
  "scripts": {
    "start": "NODE_ENV=dev webpack-dev-server --progress --config build --hot",
    "build": "npm run build:umd && npm run build:cjs",
    "build:umd": "npm run clean:umd && NODE_ENV=umd webpack --progress --config build && NODE_ENV=umd webpack --progress --config build --minify",
    "build:cjs": "npm run clean:cjs && babel src --out-dir lib --extensions '.ts'",
    "build:docs": "NODE_ENV=docs webpack --progress --config build",
    "clean:umd": "rimraf dist",
    "clean:cjs": "rimraf lib"
  },
  "keywords": [
    "upload"
  ],
  "author": "LIX",
  "license": "MIT",
  "devDependencies": {
    "@babel/cli": "^7.7.0",
    "@babel/core": "^7.7.2",
    "@babel/plugin-proposal-class-properties": "^7.7.4",
    "@babel/plugin-transform-runtime": "^7.7.4",
    "@babel/preset-env": "^7.7.1",
    "@babel/preset-react": "^7.7.4",
    "@babel/preset-typescript": "^7.7.4",
    "@types/styled-components": "^4.4.0",
    "babel-eslint": "^10.0.3",
    "core-js": "2",
    "eslint": "^6.6.0",
    "eslint-config-zcool": "^1.5.0",
    "ts-loader": "^6.2.1",
    "webpack": "^4.41.2",
    "webpack-cli": "^3.3.10",
    "webpack-dev-server": "^3.9.0",
    "webpack-merge": "^4.2.2",
    "react": "^16.12.0",
    "react-dom": "^16.12.0"
  },
  "dependencies": {
    "ali-oss": "^6.2.1",
    "spark-md5": "^3.0.0",
    "styled-components": "^4.4.1",
    "typescript": "^3.7.2"
  }
}
